coe:lozingswijze_imjv
        a                skos:Collection ;
        dc:identifier    "co_lozingswijze_imjv" ;
        skos:definition  "Lozingswijze zoals gedefinieerd in het Integraal Milieujaarverslag"@nl ;
        skos:inScheme    vlcs:emissie ;
        skos:member      cli:transfer , cli:oppompend , cli:lozend ;
        skos:prefLabel   "Collectie lozingswijzen imjv."@nl .

